{
  "name": "korner-support-service",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc && echo 'Build completed successfully!'",
    "start": "node dist/main.js",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "migrate": "knex migrate:latest --knexfile src/knexfile.ts",
    "migrate:dev": "NODE_ENV=development knex migrate:latest --knexfile src/knexfile.ts",
    "migrate:prod": "NODE_ENV=production knex migrate:latest --knexfile src/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/knexfile.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "nodemon": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
